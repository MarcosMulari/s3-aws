# Exemplo de configuração de variáveis de ambiente
# Copie este arquivo para .env e preencha com seus valores

# AWS Configuration
AWS_ACCESS_KEY_ID=sua_aws_access_key_aqui
AWS_SECRET_ACCESS_KEY=sua_aws_secret_access_key_aqui
AWS_REGION=us-east-1
S3_BUCKET_NAME=seu-bucket-name

# Server Configuration
PORT=3000
NODE_ENV=development

# S3 Configuration
PRESIGNED_URL_EXPIRATION=3600
MAX_FILE_SIZE=10485760

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000